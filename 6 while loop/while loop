Integer myNum = 1100;

while (myNum <= 2000){
    if (Math.mod(myNum, 4) == 0){
        if (Math.mod(myNum, 100) == 0){
            if(Math.mod(myNum, 400) == 0){
                System.debug(myNum + 'This is leap year');
            }
            else {
                System.debug(myNum + 'not a leap year');
            }
        }
    }
    myNum++;
}
