public class testingCode {
    
    public Integer pTotal = 0;
    public Integer rTotal = 0;
    
    public void getVal(Integer inputVal){
       pTotal = inputVal;
        rTotal = inputVal;
    }
    
    public void increamentVal(){
        pTotal++;
        rTotal++;
    }
  	
    public Integer requredP(){
        return rTotal;
    }
}
